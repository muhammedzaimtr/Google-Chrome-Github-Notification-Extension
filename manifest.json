{
  "manifest_version": 2,
  "name": "Github Notifications",
  "description": "Github Notifications",
  "version": "0.1",
  "background" : {
    "page": "index.html"
  },
  "browser_action": {
           "default_icon":  "resource/128.png",
           "default_title": "Github Notifications"
          },
          "icons": {
                     "16": "resource/16.png",
                     "32": "resource/32.png",
                     "48": "resource/48.png",
                    "128": "resource/128.png"
                   },
  "permissions": [
    "tabs",
    "https://github.com/notifications"
  ]
}
